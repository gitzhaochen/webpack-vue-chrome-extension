{
  "name": "primas-chrome-extension",
  "version": "0.0.1",
  "description": "primas-chrome-extension",
  "author": "allenzhao",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node scripts/pre-build.js && webpack --config webpack.dev.js --progress --hide-modules --hot --watch",
